<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project-mapper">
	<select id="readAll" resultType="projectDTO">
		SELECT * FROM BBS
	</select>
	
	<insert id="insertOne" parameterType="projectDTO">
		INSERT INTO BBS VALUES(#{name}, #{content}, null)
	</insert>
</mapper>